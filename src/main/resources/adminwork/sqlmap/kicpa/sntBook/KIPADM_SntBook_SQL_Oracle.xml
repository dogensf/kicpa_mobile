	<insert id="SntBookKipadmDAO.insertTaxBillItem" parameterClass="Hashmap" >
		INSERT
	  	INTO SENDBILL.TB_TAX_LINE
	       (
	           NO,
	           NO_LINE,
	           NO_ITEM,
	           MM_WRITE,
	           DD_WRITE,
	           NM_ITEM,
	           ITEM_STD,
	           QTY,
	           UM,
               AM,
               AM_VAT,
               DC_RMK,
               AMT
	       )
	       VALUES
	       (
	           #billseqNum#,
	           (SELECT NVL(MAX(NO_LINE),0)+1 FROM WHERE SENDBILL.TB_TAX_LINE NO = #billseqNum# ),
	           LTRIM(TO_CHAR((SELECT NVL(MAX(NO_LINE),0)+1 FROM WHERE SENDBILL.TB_TAX_LINE NO = #billseqNum# ),'00000000')),
	           TO_CHAR(SYSDATE,'MM'),
	           TO_CHAR(SYSDATE,'DD'),
<!-- 	           substr(#v_dt#,6,2), -->
<!-- 	           substr(#v_dt#,9,2), -->
	           #bookName#,
	           '',
	           #bookCnt#,
	           #bookAmt#,
	           #saleAmt#,
	           0,
	           '',
	           #saleAmt#
	       )

	</insert>

